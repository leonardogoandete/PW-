@startuml
Carro *-- Placa
class Carro {
    - ano : int
    - modelo : String
    - montadora : String

    + Carro()
    + Carro(int ano, String modelo, String montadora)
    + setAno(int ano)
    + getAno() : int
    + setModelo(String modelo)
    + getModelo() : String
    + setMontadora(String montadora)
    + getMontadora() : String
    + toString() : String
}

class Placa{
    - letras : String
    - numeros : int

    + Placa()
    + Placa(String letras, int numeros)
    + setLetras(String letras)
    + getLetras() : String
    + setNumeros(int numeros)
    + getNumeros() : int
    + toString() : String
}
@enduml
